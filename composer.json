{
    "name": "slash2nl/nova-back-button",
    "description": "A Laravel Nova card to display a back button.",
    "keywords": [
        "laravel",
        "nova",
        "back",
        "button"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "SLASH2NL\\NovaBackButton\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SLASH2NL\\NovaBackButton\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
